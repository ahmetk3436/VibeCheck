{
  "expo": {
    "name": "VibeCheck",
    "slug": "vibecheck",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "vibecheck",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "owner": "ahmetcoskunkizilkaya",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#030712"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.ahmetcoskunkizilkaya.vibecheck",
      "buildNumber": "1",
      "infoPlist": {
        "NSFaceIDUsageDescription": "VibeCheck uses Face ID to protect your account",
        "NSCameraUsageDescription": "VibeCheck needs camera access for future features"
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.ahmetcoskunkizilkaya.vibecheck",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#030712"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-apple-authentication",
      "expo-haptics",
      "expo-local-authentication"
    ]
  }
}
